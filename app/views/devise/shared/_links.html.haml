- if controller_name != 'sessions'
  %small= t('devise.member_already')
  = link_to t('devise.sign_in'), new_session_path(resource_name)
  %br
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to t('devise.sign_up') + " / " + t('devise.new_user'), new_registration_path(resource_name)
  %br
- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
  = link_to t('devise.forgot_pasword'), new_password_path(resource_name)
  %br
- if devise_mapping.confirmable? && controller_name != 'registrations'
  = link_to t('devise.no_confirmation_instructions'), new_confirmation_path(resource_name)
  %br
- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  = link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name)
  %br
  %br
